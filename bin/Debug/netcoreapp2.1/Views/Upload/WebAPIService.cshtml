@section SampleHeading{
    <span class="sampleName">Upload / Web API Service</span>}
@section ControlsSection{
    <div class="frame">
        <div class="control">
            <div class="posupload">Select a file to upload</div>
          <ej-upload-box id="UploadDefault" save-url="//js.syncfusion.com/demos/ejServices/api/uploadbox/Save" remove-url="//js.syncfusion.com/demos/ejServices/api/uploadbox/Remove" upload-name="Uploadbox">
         </ej-upload-box>
        </div>
    </div>
}
@section StyleSection{
    <style class="cssStyles">
        .posupload {
            padding: 10px;
            text-align: center;
        }

        #UploadDefault {
            margin: 0 auto;
        }
    </style>
}
