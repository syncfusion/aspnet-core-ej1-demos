@section SampleHeading{<span class="sampleName">TreeView / LoadonDemand </span>}
@section ControlsSection{
    <div style="width: 250px">

        <ej-tree-view id="tree" load-on-demand="true">
            <e-tree-view-fields datasource="@ViewBag.datasource" id="id" parent-id="pid" text="name" has-child="hasChild" expanded="expanded"></e-tree-view-fields>
        </ej-tree-view>
       
    </div>
}
