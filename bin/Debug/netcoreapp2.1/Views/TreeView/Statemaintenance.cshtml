@section SampleHeading{<span class="sampleName">TreeView / Statemaintenance </span>}
@section ControlsSection{
    <div style="width: 250px">

        <ej-tree-view id="tree" enable-persistence="true" width="250px">
            <e-tree-view-items>
                <e-tree-view-item text="Books" expanded="true">
                    <e-tree-view-child-items>
                        <e-tree-view-child-item text="New releases"></e-tree-view-child-item>
                        <e-tree-view-child-item text="New releases audio books"></e-tree-view-child-item>
