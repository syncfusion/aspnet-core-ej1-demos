                    </e-tree-view-child-items>
                </e-tree-view-item>
                <e-tree-view-item text="Cameras">
                    <e-tree-view-child-items>
                        <e-tree-view-child-item text="Point & Shoot"></e-tree-view-child-item>
                        <e-tree-view-child-item text="Digital SLR"></e-tree-view-child-item>
                        <e-tree-view-child-item text="Best Sellers"></e-tree-view-child-item>
                    </e-tree-view-child-items>
                </e-tree-view-item>
            </e-tree-view-items>
        </ej-tree-view> 
       
    </div>
