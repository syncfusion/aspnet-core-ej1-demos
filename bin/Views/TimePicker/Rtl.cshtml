@section MetaTags{
   <meta name="description" content="This example demonstrates how to enable the rtl property in a ASP.NET Core TimePicker">
}
@section ScriptReferenceSection{
    <script src="@Url.Content("~/Scripts/cultures/ej.culture.en-US.min.js")"></script>
}

@section SampleHeading{<span class="sampleName">TimePicker / Rtl</span> }
@section ControlsSection{
    <div class="frame">
        <div class="control" style="margin-left:30px;">
            <ej-time-picker id="TimePick" enable-rtl="true" />
        </div>
    </div>
}
