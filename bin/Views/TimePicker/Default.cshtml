@section MetaTags{
   <meta name="description" content="This example demonstrates a simple, mobile-friendly and responsive ASP.NET Core TimepIcker to select a time.">
}
@section ScriptReferenceSection{
    <script src="@Url.Content("~/Scripts/cultures/ej.culture.en-US.min.js")"></script>
}

@section SampleHeading{
    
    <span class="sampleName">TimePicker / Default </span>
}

@section ControlsSection{
    <div class="frame">
        <div class="control" style="margin-left:30px;">
            <ej-time-picker id="TimePick" />
        </div>
    </div>
}


