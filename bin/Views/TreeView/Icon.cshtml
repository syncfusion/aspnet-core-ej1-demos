@section SampleHeading{<span class="sampleName">TreeView / Icon </span>}
@section ControlsSection{
    <div style="width: 250px">

        <ej-tree-view id="treeView">
            <e-tree-view-fields datasource="@ViewBag.datasource" id="id" parent-id="pid" text="name" has-child="hasChild" expanded="expanded" sprite-css-class="spriteCss"></e-tree-view-fields>
        </ej-tree-view>
       
    </div>
}
@section StyleSection{
    <link href="@Url.Content("~/css/Samples/TreeViewIcons.css")" rel="stylesheet" />
}