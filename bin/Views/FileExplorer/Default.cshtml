@section SampleHeading{<span class="sampleName">FileExplorer / Default </span>}
@section ControlsSection{
    <ej-file-explorer id="default" path="wwwroot/css/images/FileExplorerContent" ajax-action="@Url.Content("FileActionDefault")" width="100%" is-responsive="true" layout="Tile" min-width="150" >
        <e-file-ajax-settings>
            <e-download url="/FileExplorer/Download{0}"></e-download>
            <e-get-image url="/FileExplorer/GetImage{0}"></e-get-image>
            <e-upload url="/FileExplorer/Upload{0}"></e-upload>
        </e-file-ajax-settings>
    </ej-file-explorer>
}
