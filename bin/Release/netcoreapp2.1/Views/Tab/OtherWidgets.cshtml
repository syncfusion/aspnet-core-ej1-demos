@section SampleHeading{ <span class="sampleName">Tab / OtherWidgets</span>}

@section ControlsSection{

    <div style="width: 500px">

        <ej-tab id="tabSample">
            <e-tab-items>
                <e-tab-item id="Pizza" text="Pizza Menu">
                    <e-content-template>
                        <ej-accordion id="pizzaMenu" width="460px" collapsible="true">
                            <e-accordion-items>
                                <e-accordion-item text="GARDEN FRESH (Veg)">
                                    <e-content-template>
                                        <div>
                                            <div>
                                                <div>
                                                    Rating :
                                                    <div>
                                                        <ej-rating id="gradenPizza" allow-reset="false" read-only="true" value="4" />
                                                    </div>
                                                </div>
                                                Simple and delicious mozzarella cheese and pizza sauce on a classic crust.The goodness of crunchy onions, green capsicums & tomatoes.
                                            </div>
                                        </div>
                                    </e-content-template>
                                </e-accordion-item>
                                <e-accordion-item text="CORN & SPINACH (Veg)">
                                    <e-content-template>
                                        <div>
                                            <div>
                                                Rating :
                                                <div>
                                                    <ej-rating id="cornPizza" allow-reset="false" read-only="true" value="4" />                                                   
                                                </div>
