                                        </div>
                                    </e-content-template>
                                </e-accordion-item>
                                <e-accordion-item text="CHICKEN TIKKA (Non-veg)">
                                    <e-content-template>
                                        <div>
                                            <div>
                                                Rating :
                                                <div>
                                                    <ej-rating id="chickenSand" allow-reset="false" read-only="true" value="4" />
