@section SampleHeading{
    <span class="sampleName">ListView / Data Binding Local</span>}
@section MetaTags{
    <meta name="description" content="This example demonstrates how to bind the local data source key value mapping to the Syncfusion ASP.NET Core ListView component.">
}
@model List<ListData>
@section ControlsSection{
    <ej-list-view id="locallistview" show-header="false" width="400" height="206" datasource="Model">
        <e-field-settings text="texts" />
    </ej-list-view>       
}