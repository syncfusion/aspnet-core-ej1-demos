	.cols-sample-area{
				position:relative;
			}
        #target {
            height: 320px;
            width: 600px;
        }

        #target_WaitingPopup {
            z-index: 5 !important;
        }

            #target_WaitingPopup .e-image {
                display: block;
                height: 70px;
            }
    </style>
    
}