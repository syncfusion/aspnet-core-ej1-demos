@section SampleHeading{<span class="sampleName">FileExplorer / VirtualScrolling </span>}
@section ControlsSection{
<ej-file-explorer id="VirtualScrolling" path="wwwroot/css/images/FileExplorerContent1" ajax-action="@Url.Content("FileActionDefault")" virtual-item-count="20" width="100%" is-responsive="true" layout="Tile" min-width="150">
    <e-file-ajax-settings>
        <e-download url="/FileExplorer/Download1{0}"></e-download>
        <e-get-image url="/FileExplorer/GetImage1{0}"></e-get-image>
        <e-upload url="/FileExplorer/Upload1{0}"></e-upload>
    </e-file-ajax-settings>
</ej-file-explorer>
}
