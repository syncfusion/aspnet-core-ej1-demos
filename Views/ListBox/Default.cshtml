@section SampleHeading{
    <span class="sampleName">ListBox / Default Functionalities</span>}
@section MetaTags{
    <meta name="description" content="This example demonstrates how to display a list of items using the Syncfusion ASP.NET Core ListBox component.">
}
@section ControlsSection{
        <div class="control frame">
            <div class="ctrllabel">

                Select a car</div>
            <ej-list-box id="list1" height="220px" target-id="carsList"></ej-list-box>            
                <ul id="carsList">
                    <li>Audi A4</li>
                    <li>Audi A5</li>
                    <li>Audi A6</li>
                    <li>Audi A7</li>
                    <li>Audi A8</li>
                    <li>BMW 501</li>
                    <li>BMW 502</li>
                    <li>BMW 503</li>
                    <li>BMW 507</li>
                    <li>BMW 3200</li>
                </ul>
        </div>
}
@section StyleSection{
    <style type="text/css" class="cssStyles">
        .ctrllabel {
            padding: 0px 0px 12px 3px;
            font-weight: 600;
        }
        .e-ddl-popup {
            margin:0 auto;
        }
    </style>
}